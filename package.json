{
  "name": "pagespace-markdown",
  "version": "1.0.2",
  "description": "Markdwon plugin for Pagespace",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagespace/pagespace-markdown"
  },
  "main": "index.js",
  "scripts": {
    "start": "node standalone"
  },
  "author": "Phil Mander",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "serve-favicon": "^2.3.0"
  },
  "keywords" : [ "pagespace-plugin"],
  "pagespace": {
    "config": {
      "md": "# New markdown",
      "wrapperClass": "webcopy"
    },
    "schema" : [{
        "name" : "md",
        "title" : "Markdown",
        "default" : "# New markdown",
        "type" : "string",
        "description" : "Markdown content"
    }, {
        "name" : "wrapperClass",
        "title" : "Wrapper Class",
        "default" : "webcopy",
        "type" : "string"
    }]
  },
  "dependencies": {
    "bluebird": "^2.10.2",
    "highlight.js": "^8.8.0",
    "marked": "^0.3.5"
  }
}
